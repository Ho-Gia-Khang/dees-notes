generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model File {
  id         String   @id @default(cuid())
  name       String
  size       Int
  type       String
  userId     String
  uploadedAt DateTime @default(now())
  uploadedBy String
}

// You can add more models here as needed for your application
