generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model File {
  id         String   @id @default(cuid())
  name       String
  size       Int
  extension  String
  type       EFileType
  userId     String
  uploadedAt DateTime @default(now())
  uploadedBy String
}

enum EFileType {
  MEDIA
  DOCUMENT
}